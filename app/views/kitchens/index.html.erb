<h1>Kitchens</h1>

<div class="wrapper">
	
	<% @kitchens.each do |kitchen| %>
		<%= kitchen.name %>
		<%= link_to 'Show', kitchen %>
		<%= link_to 'Edit', edit_kitchen_path(kitchen) %>
		<%= link_to 'Destroy', kitchen, method: :delete, data: { confirm: 'Are you sure?' } %>
		<br>
	<% end %>
	
</div>

<br />

<%= link_to 'New Kitchen', new_kitchen_path %>
